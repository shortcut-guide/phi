{
    "name": "phi-admin",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "clean": "rimraf public",
        "node:build": "node esbuild.config.mjs",
        "node:dev": "dotenv -e .env.development -- nodemon --ext ts --ignore public/ --exec \"tsc && node public/server.js\"",
        "node:start": "npm run clean && npm run node:build && dotenv -e .env.production -- node public/server.js",
        "d1:build": "npm run clean && node --experimental-json-modules esbuild.config.mjs",
        "d1:start": "npm run d1:build && dotenv -e .env.production -- node public/d1Server.js",
        "d1:dev": "npx tsx scripts/dev.mjs",
        "d1:deploy:products": "cd src/d1-worker/products && wrangler deploy --config wrangler-production.toml",
        "d1:deploy:searchlogs": "cd src/d1-worker/searchlogs && wrangler deploy --config wrangler-production.toml",
        "d1:deploy:profile": "cd src/d1-worker/profile && wrangler deploy --config wrangler-production.toml",
        "all:deploy": "npm run node:start && npm run d1:start && npm run d1:deploy:products && npm run d1:deploy:searchlogs && npm run d1:deploy:profile",
        "test": "node --max-old-space-size=4096 --experimental-vm-modules node_modules/.bin/jest --config ./jest.config.mjs"
    },
    "dependencies": {
        "@hono/node-server": "^1.14.1",
        "@prisma/client": "^6.5.0",
        "@tsndr/cloudflare-worker-jwt": "^3.2.0",
        "@vercel/node": "^2.3.0",
        "axios": "^1.8.1",
        "better-sqlite3": "^12.2.0",
        "body-parser": "^1.20.0",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "crypto": "^1.0.1",
        "csurf": "^1.11.0",
        "dayjs": "^1.11.13",
        "dotenv": "^17.2.0",
        "express": "^4.21.2",
        "glob": "^9.3.5",
        "googleapis": "^148.0.0",
        "hono": "^4.7.4",
        "http-proxy-middleware": "^3.0.5",
        "install": "^0.13.0",
        "ioredis": "^5.6.1",
        "itty-router": "^5.0.18",
        "jsonwebtoken": "^9.0.2",
        "kysely": "^0.27.6",
        "kysely-d1": "^0.3.0",
        "mongoose": "^6.8.1",
        "node-fetch": "^2.7.0",
        "nodemailer": "^6.10.1",
        "npm": "^11.4.2",
        "puppeteer": "^24.5.0",
        "rimraf": "^4.4.1",
        "tailwindcss": "^4.0.9",
        "yarn": "^1.22.22",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@babel/core": "^7.26.10",
        "@babel/preset-env": "^7.26.9",
        "@cloudflare/workers-types": "^4.20250715.0",
        "@mermaid-js/mermaid-cli": "^11.4.2",
        "@miniflare/d1": "^2.14.4",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.14",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node-fetch": "^2.6.12",
        "@types/uuid": "^10.0.0",
        "astro": "^5.9.1",
        "babel-jest": "^29.7.0",
        "concurrently": "^9.2.0",
        "dotenv-cli": "^8.0.0",
        "esbuild": "^0.25.5",
        "esbuild-plugin-alias": "^0.2.1",
        "esbuild-register": "^3.6.0",
        "eslint": "^8.0.0",
        "husky": "^8.0.0",
        "jest": "^29.7.0",
        "jest-fetch-mock": "^3.0.3",
        "nodemon": "^3.1.9",
        "prettier": "^3.0.0",
        "prisma": "^6.5.0",
        "prisma-kysely": "^1.8.0",
        "puppeteer": "^23.11.1",
        "supertest": "^7.1.0",
        "ts-jest": "^29.3.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.20.3",
        "ttypescript": "^1.5.15",
        "typescript": "^5.8.3",
        "typescript-transform-paths": "^3.5.5",
        "wrangler": "^4.22.0"
    }
}
