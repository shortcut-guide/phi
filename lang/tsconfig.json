{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",      
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./public"
  },
  "include": ["src"]
}